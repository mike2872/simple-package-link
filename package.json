  "engines": {
    "node": "^16 || ^18",
    "pnpm": ">=7"
  },
  "bin": {
    "spl-linked": "./lib/scripts/linked.js",
    "spl-unlinked": "./lib/scripts/unlinked.js"
  },
  "scripts": {
    "test": "pnpm run lint && pnpm run compile && pnpm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "jest": "jest --forceExit",
    "compile": "shx rm -rf lib/ && tsc",
    "prepare": "husky install",
    "prepublish": "pnpm compile",
    "publish:latest": "pnpm publish --tag latest --access public --publish-branch main --no-git-checks"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "dependencies": {
    "concurrently": "^7.2.1",
